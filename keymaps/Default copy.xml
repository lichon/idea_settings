    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="Debug">
    <keyboard-shortcut first-keystroke="shift f9" />
    <keyboard-shortcut first-keystroke="shift alt d" />
  </action>
  <action id="EditorSelectWord" />
  <action id="EmmetNextEditPoint" />
  <action id="EmmetPreviousEditPoint" />
  <action id="FindNext">
    <keyboard-shortcut first-keystroke="ctrl l" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
    <mouse-shortcut keystroke="control button1" />
    <mouse-shortcut keystroke="button2" />
    <keyboard-shortcut first-keystroke="f3" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="alt right" />
